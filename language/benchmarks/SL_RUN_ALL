#!/bin/bash

CMDS="
Append
Reverse
Map
Filter
"

for cmd in $CMDS; do
    sum=0
    count=$1
    for ((i=1;i<=count;i++)); do
        result=`./SL_RUN $cmd`
        time=`awk 'END {print $NF}' <<< "$result"`
        echo $i $time
        sum=`expr $sum + $time`
    done

    avg=`expr $sum / $count`
    echo $cmd $avg
done

